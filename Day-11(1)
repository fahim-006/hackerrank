#include <bits/stdc++.h>
using namespace std;
int main(){
	int a[8][8],i,j,sum[50]={0},max=-100;
	
	for(i=0; i<6; i++){
		for(j=0; j<6; j++){
			scanf("%d", &a[i][j]);
		}
	}
	sum[1]=a[0][0]+a[0][1]+a[0][2]+a[1][1]+a[2][0]+a[2][1]+a[2][2];
	sum[2]=a[0][1]+a[0][2]+a[0][3]+a[1][2]+a[2][1]+a[2][2]+a[2][3];
	sum[3]=a[0][2]+a[0][3]+a[0][4]+a[1][3]+a[2][2]+a[2][3]+a[2][4];
	sum[4]=a[0][3]+a[0][4]+a[0][5]+a[1][4]+a[2][3]+a[2][4]+a[2][5];
	sum[5]=a[1][0]+a[1][1]+a[1][2]+a[2][1]+a[3][0]+a[3][1]+a[3][2];
	sum[6]=a[1][1]+a[1][2]+a[1][3]+a[2][2]+a[3][1]+a[3][2]+a[3][3];
	sum[7]=a[1][2]+a[1][3]+a[1][4]+a[2][3]+a[3][2]+a[3][3]+a[3][4];
	sum[8]=a[1][3]+a[1][4]+a[1][5]+a[2][4]+a[3][3]+a[3][4]+a[3][5];
	sum[9]=a[2][0]+a[2][1]+a[2][2]+a[3][1]+a[4][0]+a[4][1]+a[4][2];
	sum[10]=a[2][1]+a[2][2]+a[2][3]+a[3][2]+a[4][1]+a[4][2]+a[4][3];
	sum[11]=a[2][2]+a[2][3]+a[2][4]+a[3][3]+a[4][2]+a[4][3]+a[4][4];
	sum[12]=a[2][3]+a[2][4]+a[2][5]+a[3][4]+a[4][3]+a[4][4]+a[4][5];
	sum[13]=a[3][0]+a[3][1]+a[3][2]+a[4][1]+a[5][0]+a[5][1]+a[5][2];
	sum[14]=a[3][1]+a[3][2]+a[3][3]+a[4][2]+a[5][1]+a[5][2]+a[5][3];
	sum[15]=a[3][2]+a[3][3]+a[3][4]+a[4][3]+a[5][2]+a[5][3]+a[5][4];
	sum[16]=a[3][3]+a[3][4]+a[3][5]+a[4][4]+a[5][3]+a[5][4]+a[5][5];
	
	for(int k=1; k<=16; k++){
		if(max<sum[k])
			max=sum[k];
	}
	printf("%d\n",max);
	return 0;
	
}
